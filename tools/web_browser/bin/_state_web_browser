#!/usr/bin/env python3

import json
import os
from pathlib import Path


def main():
    """State command for web_browser tool bundle."""
    state = {
        "working_dir": os.getcwd(),
        "tool_type": "web_browser",
        "browser_active": True  # Assume browser is active when this tool is loaded
    }
    
    print(json.dumps(state))


if __name__ == "__main__":
    main() 